package neww;
import javax.swing.*;
import java.awt.*;
class guitest{
    public static void main(String args[]){
       JFrame frame = new JFrame("lulz");
       frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
       frame.setSize(500,450);
       JMenuBar mb=new JMenuBar();
       JMenu m1=new JMenu("File");
       JMenu m2=new JMenu("tem pas");
       JMenuItem m11=new JMenuItem("wew");
       JMenuItem m22=new JMenuItem("dusra");
       mb.add(m1);
       mb.add(m2);
       JButton button = new JButton("bak");
       JButton nui=new JButton("nui");
       frame.getContentPane().add(button); // Adds Button to content pane of frame
       frame.getContentPane().add(nui);
       
       frame.setVisible(true);
    }
}