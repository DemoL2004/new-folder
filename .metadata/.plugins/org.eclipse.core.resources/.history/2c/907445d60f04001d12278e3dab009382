package p1.p3;

import java.util.Scanner;

public class Person {
		String name;
		int aadhar;
		
		void setdata(){
			Scanner n=new Scanner(System.in);
			System.out.println("enter Name");
			String b=n.nextLine();
			System.out.println("enter aadhar");
			int a=n.nextInt();

			aadhar=a;
			name=b;
		}
	}

