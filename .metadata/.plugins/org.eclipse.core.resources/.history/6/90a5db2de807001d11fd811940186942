package autoboxx;

public class box {
	public static void main(String args[]){
		byte b=10;
		short s=20;
		long l=40;
		int i=50;
		
		//AutoBoxing converting primitives to objects
		Byte ByteObj=b;
		Short ShortObj=s;
		Long LongObj=l;
		Integer IntObj=i;
		
		System.out.println("_________PRINTING THE OBJECT VALUES________");
		System.out.println("Byte Object = " +ByteObj);
		System.out.println("short object = "+ShortObj);
		System.out.println("Long object = "+LongObj);
		System.out.println("Int object = "+IntObj);
		
		Float FloatObj=50.0F;
		Double DoubleObj=60.80;
		Character CharObj= 'H';
		Boolean BoolObj=true;
		String StringObj="Hi";
		
		float f=FloatObj;
		double d=DoubleObj;
		char c=CharObj;
		boolean boo=BoolObj;
		String ss=StringObj;
		
		System.out.println("Float value ="+f);
		System.out.println("Double value = "+d);
		System.out.println("Char value ="+StringObj);
		System.out.println("Bool value = "+boo);
	}
}
